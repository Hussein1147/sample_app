<!DOCTYPE html>
       <html>
       <head>
     	  <title><%= title %></title>
		  <%= csrf_meta_tag %>
		  <%= render 'layouts/stylesheets'%>
		  <%= favicon_link_tag 'favicon.ico' %>
		 
        </head>
		<body >
		   <div class="container">
			
		        <%=render 'layouts/header' %>
		        <section class="round">
				<% flash.each do |key, value|%>
					<div class="flash <%= key %>"> <%= value %></div>
				<% end %>
   		          <%= yield %>
		        </section>
	     <%= render 'layouts/footer'%>
		 <%= d params %>
		   
	       </div>
        </body>
  </html>